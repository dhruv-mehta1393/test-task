<table class="table">
<h1>User List</h1>
  <thead>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">Email Address</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>  
    <tr >
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Edit User Status', edit_user_path(user) %></td>
    </tr>
    <% end %>
  </tbody>
</table>